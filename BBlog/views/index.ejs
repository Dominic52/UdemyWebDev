<% include partials/header %>
<h1>THIS IS INDEX</h1>

<% blogs.forEach(function(blog){ %>
    <div class="ui segment">
        <h2><%=blog.title%></h2>
        <img src="<%=blog.image%>" class="ui medium image">
        <span><%=blog.created%></span>
        <p><%=blog.body%></p>
        <a href="/blogs/<%=blog._id%>">Read More</a>
    </div>

<% }); %>

<% include partials/footer %>